{
  "name": "iqiyi-integral",
  "version": "1.0.0",
  "private": true,
  "typings": "typings/index",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "bundle:android": "qrn_pack_new unbundle --entry-file index.android.js --bundle-output ./bundle/android/IntegralRN_android.bundle --id-suffix 10 --sourcemap-output ./bundle/android/IntegralRN_android.map --assets-dest ./bundle/android --platform android --dev false --reset-cache",
    "bundle:ios": "qrn_pack_new unbundle --entry-file index.ios.js --bundle-output ./bundle/ios/IntegralRN.jsbundle --id-suffix 10 --sourcemap-output ./bundle/ios/IntegralRN.map --assets-dest ./bundle/ios --platform ios --dev false --reset-cache",
    "lint": "eslint src",
    "mockserver": "nodemon --watch mocks ./commands/MockServer.js",
    "mock": "run-p mockserver start",
    "cmock": "node ./commands/CreateMockFile",
    "atcp": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:3001 tcp:3001",
    "cpd": "jscpd ./src -c .jscpd.json; open ./report/jscpd/jscpd-report.html"
  },

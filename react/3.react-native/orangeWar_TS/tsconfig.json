{
    "compilerOptions": {
        "lib": [
            "es2015",
            "es2016",
            "es2017",
            "es2018"
        ],
        "target": "ES2015",
        "module": "es2015",
        "jsx": "react",
        "noEmit": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "alwaysStrict": false,
        "esModuleInterop": true,
        "sourceRoot": "src",
        "sourceMap": true,
        "noImplicitAny": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": false,
        "experimentalDecorators": true
    },
    "include": [
    ],
    "exclude": [
        "node_modules"
    ]
}

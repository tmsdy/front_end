{
    "name": "iqiyi-integral-h5",
    "version": "1.0.0",
    "private": true,
    "config": {
        "vd": "/integralh5",
        "port": 3000,
        "env": "fat"
    },
    "browserslist": [
        "last 2 versions",
        "> 1% in CH",
        "not ie <= 8"
    ],
    "scripts": {
        "start": "cross-env NODE_ENV=development NODE_PORT=3000 nodemon ./_server/bin/www --watch routes --watch _server",
        "inspect": "cross-env NODE_ENV=development NODE_PORT=3000 nodemon --inspect ./_server/bin/www --watch _server",
        "start:prod": "cross-env NODE_ENV=production NODE_PORT=3000 node ./_server/bin/www",
        "start:prod:build": "npm run build && cross-env NODE_ENV=production BUILD=1 PORT=3001 node ./_server/bin/www",
        "build": "cross-env NODE_ENV=production webpack --config ./_server/build/webpack/prod.conf.js --progress --colors --hide-modules",
        "build:test": "cross-env NODE_ENV=test webpack --config ./_server/build/webpack/test.conf.js --progress --colors --hide-modules",
        "eslint": "eslint src",
        "prebuild": "rimraf ./dest"
    },
    "dependencies": {
        "@iqiyi/callordownloadapp": "^1.0.20",
        "app-root-path": "^2.1.0",
        "autoprefixer": "^9.0.1",
        "babel-core": "^6.3.26",
        "babel-loader": "^7.1.5",
        "babel-plugin-transform-es2015-for-of": "^6.23.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-stage-0": "^6.5.0",
        "babel-register": "^6.14.0",
        "body-parser": "~1.13.2",
        "chalk": "^1.1.3",
        "compare-versions": "^3.4.0",
        "compression": "^1.7.3",
        "cookie-parser": "~1.3.5",
        "copy-webpack-plugin": "^4.5.2",
        "core-js": "^3.0.0-beta.3",
        "cross-env": "^2.0.1",
        "css-loader": "~0.25.0",
        "cssnano": "^4.0.5",
        "debug": "~2.2.0",
        "dom-to-image": "^2.6.0",
        "ejs": "~2.3.3",
        "eruda": "^1.5.4",
        "expose-loader": "^0.7.3",
        "express": "~4.14.0",
        "file-loader": "^1.1.11",
        "fs-extra": "^6.0.1",
        "helmet": "^3.13.0",
        "iphone-inline-video": "^2.2.2",
        "json-bigint": "^0.2.3",
        "less": "^3.8.0",
        "less-loader": "^4.1.0",
        "log": "^1.4.0",
        "mini-css-extract-plugin": "^0.4.1",
        "morgan": "~1.6.1",
        "node-fetch": "^1.6.3",
        "nodemon": "1.11.0",
        "optimize-css-assets-webpack-plugin": "^5.0.0",
        "optimize-js-plugin": "^0.0.4",
        "pm2": "2.4.6",
        "postcss-loader": "^2.1.6",
        "request": "^2.87.0",
        "rimraf": "^2.6.1",
        "style-loader": "^0.21.0",
        "uglifyjs-webpack-plugin": "^1.2.7",
        "url-loader": "^1.0.1",
        "url-parse": "^1.1.9",
        "vconsole": "^3.0.0",
        "webpack": "^4.16.2",
        "webpack-cli": "^3.1.0",
        "webpack-dev-middleware": "^3.1.3",
        "webpack-merge": "^4.1.3",
        "xml2js": "^0.4.19",
        "zepto": "^1.2.0"
    },
    "devDependencies": {
        "babel-eslint": "^10.0.1",
        "eslint": "^5.15.3",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-import-resolver-webpack": "^0.11.0",
        "eslint-plugin-import": "^2.16.0",
        "webpack-bundle-analyzer": "^2.13.1"
    }
}
